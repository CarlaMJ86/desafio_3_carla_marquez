<h1>Movies#new</h1>
<p>Find me in app/views/movies/new.html.erb</p>
<h1>Nueva Movie</h1>
<%= form_with url: movies_create_path, model: @movie, method: :post do |form| %>
  <div>
    <%= form.label :name, "Ingresa Tu Movie" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :synopsis, "Cuentame de tu Movie" %>
    <%= form.text_area :synopsis %>
  </div>
  <div>
    <%= form.label :director, "Dime el Director" %>
    <%= form.text_field :director %>
  </div>
  <div>
    <%= form.submit "Guardar" %>
  </div>
<% end %>